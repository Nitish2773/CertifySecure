*.yml text eol=lf
*.dart text eol=lf
*.js text eol=lf
*.ts text eol=lf
*.html text eol=lf
*.css text eol=lf
*.md text eol=lf
*.gradle text eol=lf
*.properties text eol=lf
*.xml text eol=lf
*.txt text eol=lf
# Specific files
pubspec.lock text eol=lf
